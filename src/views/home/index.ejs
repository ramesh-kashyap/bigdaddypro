                                    console.error('Login failed:', response.message);
                                    // Handle failure (e.g., show an error message to the user)
                                }
                            },
                            error: function(xhr, status, error) {
                                console.error('Error occurred while logging in:', error);
                                // Handle general errors (e.g., network issues)
                            }
                        });

                    } else {
                        // Show the popup if balance is insufficient
                        showPopup();
                    }
                },
                error: function(xhr, status, error) {
                    console.error('Error occurred while getting user info:', error);
                    // Handle general errors (e.g., network issues)
                }
            });
        }

        // Attach event handlers to the popup buttons
        $(document).on('click', '.van-dialog__cancel', function() {
            hidePopup();
        });

