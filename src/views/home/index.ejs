            document.getElementById('myPopup').style.display = 'none';
        }

        // Function to handle confirmation
        function handleConfirmation() {
            // Call the /api/webapi/GetUserInfo endpoint to get user information
            $.ajax({
                type: 'GET',
                url: '/api/webapi/GetUserInfo',
                success: function(userInfo) {
                    console.log(userInfo.data);
                    const { money_user, thirdparty_wallet } = userInfo.data;

                    if (money_user > 0) {
                        // Call the /aviatorMoneySend endpoint if the user has money
                        $.ajax({
                            type: 'POST',
                            url: '/aviatorMoneySend',
                            success: function(response) {
                                if (response.status) {
                                    console.log('Balance sent successfully:', response);

                                    // After successfully sending the balance, call the /aviatorgame endpoint with the gameId
                                    $.ajax({
                                        type: 'POST',
                                        url: `/aviatorgame/${gameId}`,
                                        success: function(response) {
                                            if (response.status) {
                                                console.log('Login successful:', response);
                                                // Check if ErrorCode is 0
                                                if (response.data.ErrorCode === 0) {
                                                    // Open the URL from response.data.Data
													window.open(response.data.Data, '_blank');
                                                } else {
                                                    console.error('ErrorCode is not 0:', response.data.ErrorCode);
                                                    // Handle other ErrorCode values
                                                }
                                            } else {
                                                console.error('Login failed:', response.message);
                                                // Handle failure (e.g., show an error message to the user)
                                            }
                                        },
                                        error: function(xhr, status, error) {
                                            console.error('Error occurred while logging in:', error);
                                            // Handle general errors (e.g., network issues)
                                        }
                                    });

                                } else {
                                    console.error('Failed to send balance:', response.message);
                                    // Handle failure (e.g., show an error message to the user)
                                }
                            },
                            error: function(xhr, status, error) {
                                console.error('Error occurred while sending balance:', error);
                                // Handle general errors (e.g., network issues)
                            }
                        });

                    } else if (thirdparty_wallet > 0) {
                        // Directly call the /aviatorgame endpoint if thirdparty_wallet > 0
                        $.ajax({
                            type: 'POST',
                            url: `/aviatorgame/${gameId}`,
                            success: function(response) {
                                if (response.status) {
                                    console.log('Login successful:', response);
                                    // Check if ErrorCode is 0
